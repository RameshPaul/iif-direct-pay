            <column name="payment_destination_source" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_received_date_t_ime" type="date">
                <constraints nullable="true" />
            </column>
            <column name="payment_received_status" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_received_details" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_refunded_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="user_comment" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="flagged_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="flag_details" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="flagged_email_id" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="flagged_amount" type="double">
                <constraints nullable="true" />
            </column>
            <column name="flag_created_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="is_recurring_payment" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="transaction_details" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="created_date" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="updated_date" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="patron_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="patron_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="patron_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="receipient_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="receipient_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="receipient_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="payment_source_account_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="payment_destination_account_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="flagged_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="flagged_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="flag_cleared_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="flag_cleared_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="payment" columnName="created_date" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="payment" columnName="updated_date" columnDataType="${datetimeType}"/>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

    <!--

            </column>
            <column name="amount" type="double">
                <constraints nullable="true" />
            </column>
            <column name="transaction_id" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_source" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_status" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_status_details" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="payment_start_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="payment_complete_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="payment_failure_date_time" type="date">
                <constraints nullable="true" />
            </column>
            <column name="patron_comment" type="varchar(255)">

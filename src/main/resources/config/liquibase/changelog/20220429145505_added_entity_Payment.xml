            <column name="payment_source" type="string"/>
            <column name="payment_status" type="string"/>
            <column name="payment_status_details" type="string"/>
            <column name="payment_start_date_time" type="date"/>
            <column name="payment_complete_date_time" type="date"/>
            <column name="payment_failure_date_time" type="date"/>
            <column name="patron_comment" type="string"/>
            <column name="is_auto_pay" type="boolean"/>
            <column name="payment_destination_source" type="string"/>
            <column name="payment_received_date_t_ime" type="date"/>
            <column name="payment_received_status" type="string"/>
            <column name="payment_received_details" type="string"/>
            <column name="payment_refunded_date_time" type="date"/>
            <column name="user_comment" type="string"/>
            <column name="flagged_date_time" type="date"/>
            <column name="flag_details" type="string"/>

        <dropDefaultValue tableName="receipient" columnName="created_date" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="receipient" columnName="updated_date" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="receipient" columnName="deleted_date" columnDataType="${datetimeType}"/>
    </changeSet>


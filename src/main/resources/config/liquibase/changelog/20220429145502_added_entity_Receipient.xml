            <column name="approved_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="approved_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="rejected_user_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="rejected_user_org_id_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->

            <column name="onboarded_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="reccuring_pause_date" type="date">
                <constraints nullable="true" />
            </column>
            <column name="recurring_resume_date" type="date">
                <constraints nullable="true" />
